<vector
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aapt="http://schemas.android.com/aapt"
android:width="240dp"
android:height="240dp"
android:viewportWidth="240"
android:viewportHeight="240"
    >
<group>
    <clip-path
        android:pathData="M0 0H240V240H0V0Z"
        />
    <group
        android:translateX="1"
        android:translateY="0"
        android:pivotX="120"
        android:pivotY="120"
        android:scaleX="2"
        android:scaleY="2"
        android:rotation="90"
        >
        <path
            android:pathData="M0 0V240H240V0"
            >
            <aapt:attr name="android:fillColor">
                <gradient
                    android:type="linear"
                    android:startX="60"
                    android:startY="120"
                    android:endX="180"
                    android:endY="120"
                    >
                    <item
                        android:color="#6E8AFC"
                        android:offset="0"
                        />
                    <item
                        android:color="#375DFB"
                        android:offset="1"
                        />
                </gradient>
            </aapt:attr>
        </path>
    </group>
</group>
</vector>